Package: isopam
Type: Package
Title: Clustering of Sites with Species Data
Version: 1.2.0
Authors@R: c(person("Sebastian", "Schmidtlein", role = c("aut", "cre"),
                   email = "schmidtlein@kit.edu",
                   comment = c(ORCID = "0000-0003-1888-1865")),
